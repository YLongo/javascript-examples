{
  "name": "hello-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:cjs": "node src/cjs/index.cjs",
    "dev:mjs": "node src/esm/index.mjs",
    "serve": "node server/index.js",
    "compile": "babel src/babel --out-dir compiled",
    "dev:ts": "npx tsx src/ts/index.ts",
    "build": "tsc src/ts/index.ts --esModuleInterop  --skipLibCheck --outDir dist --target es6",
    "build-mjs": "esbuild src/ts/*.ts --outdir=dist --out-extension:.js=.mjs --format=esm"

  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@types/md5": "^2.3.2",
    "@types/node": "^24.0.14",
    "esbuild": "^0.25.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^4.9.5"
  }
}
